{
  "author": "NetScout-Go",
  "description": "Scans for open ports on a target host",
  "icon": "search",
  "id": "port_scanner",
  "license": "MIT",
  "name": "Port Scanner",
  "parameters": [
    {
      "description": "The hostname or IP address to scan",
      "id": "host",
      "name": "Host",
      "required": true,
      "type": "string"
    },
    {
      "default": "1-1024",
      "description": "Range of ports to scan (e.g., 22-80)",
      "id": "portRange",
      "name": "Port Range",
      "required": true,
      "type": "string"
    },
    {
      "default": 1,
      "description": "Timeout in seconds for each port",
      "id": "timeout",
      "max": 10,
      "min": 0.1,
      "name": "Timeout",
      "required": false,
      "step": 0.1,
      "type": "number"
    }
  ],
  "repository": "https://github.com/NetScout-Go/Plugin_port_scanner",
  "version": "1.0.0"
}